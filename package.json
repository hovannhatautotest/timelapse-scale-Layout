{
  "name": "admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "npx serve dist/admin -l 4200",
    "build:prod": "ng build --configuration production && npm run format:build",
    "build:stag": "ng build --configuration staging && npm run format:build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,html,less}\"",
    "format:build": "prettier --write \"dist/**/*.html\" --print-width=999999",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "test-storybook": "test-storybook",
    "chromatic": "node --max_old_space_size=4096 && chromatic --exit-zero-on-changes",
    "update-all": "npx -p npm-check-updates ncu -u && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.10",
    "@angular/common": "^16.2.10",
    "@angular/compiler": "^16.2.10",
    "@angular/core": "^16.2.10",
    "@angular/forms": "^16.2.10",
    "@angular/platform-browser": "^16.2.10",
    "@angular/platform-browser-dynamic": "^16.2.10",
    "@angular/router": "^16.2.10",
    "@angular/service-worker": "^16.2.10",
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/embed": "^2.6.0",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.2",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@editorjs/table": "^2.2.2",
    "@editorjs/underline": "^1.1.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-moment": "^1.3.0",
    "codex-notifier": "^1.1.2",
    "codex-tooltip": "^1.0.5",
    "dayjs": "^1.11.10",
    "draggabilly": "^3.0.0",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "glightbox": "^3.2.0",
    "gsap": "^3.12.2",
    "line-awesome": "^1.3.0",
    "ng-zorro-antd": "16.2.0",
    "ngx-color-picker": "^15.0.0",
    "ngx-echarts": "^16.1.1",
    "ngx-mask": "^16.3.9",
    "ngx-suneditor": "^1.1.2",
    "rxjs": "~7.8.1",
    "slug": "^8.2.3",
    "suneditor": "^2.45.1",
    "sweetalert2": "^11.7.32",
    "swiper": "^11.0.3",
    "tslib": "^2.6.2",
    "vanilla-lazyload": "^17.8.5",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.7",
    "@angular/cli": "~16.2.7",
    "@angular/compiler-cli": "^16.2.10",
    "@babel/core": "^7.23.2",
    "@types/draggabilly": "^2.1.6",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.8.7",
    "@types/slug": "^5.0.6",
    "@types/swiper": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-loader": "^9.1.3",
    "eslint": "8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.9.5"
  }
}
